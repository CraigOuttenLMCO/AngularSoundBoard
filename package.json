{
  "name": "angular-sound-board",
  "version": "0.0.12",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-lm": "ng serve --host localhost --port 80",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "package:win": "ng build --prod && cross-var electron-packager . $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite",
    "package:linux": "ng build --prod && cross-var electron-packager . $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite",
    "package:all": "ng build --prod && cross-var electron-packager . $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.3",
    "@angular/cdk": "^6.4.6",
    "@angular/common": "^6.1.3",
    "@angular/compiler": "^6.1.3",
    "@angular/core": "^6.1.3",
    "@angular/forms": "^6.1.3",
    "@angular/http": "^6.1.3",
    "@angular/material": "^6.4.6",
    "@angular/platform-browser": "^6.1.3",
    "@angular/platform-browser-dynamic": "^6.1.3",
    "@angular/router": "^6.1.3",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "module-js": "^7.0.1",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.1.4",
    "@angular/compiler-cli": "^6.1.3",
    "@angular/language-service": "^6.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cross-var": "^1.1.0",
    "electron": "^2.0.7",
    "electron-packager": "^12.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
